/* ELEMENTS */

figure {
    
    display: inline-block;
    text-align:center;
    max-width: 100%;    
    margin: 1em 1em 1em;
    
    img, object {
        max-width: 100%;
        height: auto;
        vertical-align: top;
    }
    
    figcaption {
        font-size: $smaller;
        font-weight: 300;
        opacity: 1 !important;
        width: 100%;
        display: block;
        padding-top: 1em;
        text-align: center;
        color: $grey;
        .right & {
            text-align:right;
            margin-right: 0px !important;
            overflow:visible;
            white-space:nowrap;
            }
        .left & {
            text-align:left;
            margin-left: 0px !important;
            overflow:visible;
            white-space:nowrap;
            }
        }
    
    .note {        
        
        position: absolute;
        left: 32.82999999%;
        bottom: 105%;
        width: 35%;
        text-align: center;
        padding: $smaller $big;
        -webkit-border-radius: 3px;
        border-radius: 3px;
        
        font-family: 'Bad Script', cursive;
        font-size: $small;
        line-height: $bigger;
        color: $colorNoteText;
        
        opacity: 0;
        
        background: $colorNoteBg;
        
        -webkit-transition: $slideAnimation;
        -moz-transition: $slideAnimation;
        -o-transition: $slideAnimation;
        -ms-transition: $slideAnimation;
        transition: $slideAnimation;
        
        span {
            float:center;
            width: auto;
            max-width: 76%;
            }
        &:after {
            content: "";
            position: absolute;
            width: 0;
            height: 0;
            border-width: 15px;
            border-style: solid;
            border-color: $colorNoteBg transparent transparent transparent ;
            top: 100%;
            left: 45%;
            }
        &.visible {
            opacity: 1;
            bottom: 97%
            }
        }
    .note-final {
        opacity: 0;
        }
    
    /* Crop */
    &.figure-crop {
        text-align: center;
        img {
            width: 100%;
            }
        }
    
    /* Interactive Figures */
    
    [foo], &.figure-overlay {
        position: relative;
        text-align:left;
        
        &, img:first-child, object:first-child {
            opacity: 1 !important;
            }
        &>img:nth-child(n+2), object:nth-child(n+2) {
            position: absolute;
            top: 0;
            left: 0;
            opacity: 0;
            -webkit-transition: $fadeOpacity;
            -moz-transition: $fadeOpacity;
            -o-transition: $fadeOpacity;
            -ms-transition: $fadeOpacity;
            transition: $fadeOpacity;
            display: block;
            } 
        img.allways-visible {
            opacity: 1 !important;
            display: block !important;    
            }
        img:hover {
            cursor: pointer;
            }
        }
    
    [foo], &.figure-overlay-in-device {
        position: relative;
        text-align:left;
        margin-bottom: $large;
        
        
        &>img, &>object {
            position: absolute;
            top: 0;
            left: 0;
            -webkit-transition: $fadeOpacity;
            -moz-transition: $fadeOpacity;
            -o-transition: $fadeOpacity;
            -ms-transition: $fadeOpacity;
            transition: $fadeOpacity;
            display: block;
            } 
        &>.device {
            position: static; 
            }
        div.device-desktop-content {
            background-color: $black;
            color: $grey;
            padding:0;     
            margin:0;     
            overflow: visible !important;
            position: absolute;
            top: 4.9%;
            left: 4%;
            width: 92.1%;
            height: auto;
            
            & > img:first-child {
                position: static;    
                }    
                
            &>img, object {
                position: absolute;
                top: 0px;
                left: 0px;
                opacity: 0;
                -webkit-transition: $fadeSlowOpacity;
                -moz-transition: $fadeSlowOpacity;
                -o-transition: $fadeSlowOpacity;
                -ms-transition: $fadeSlowOpacity;
                transition: $fadeSlowOpacity;
                }
            &>img.visible, &>object.visible {
                opacity: 1;
                }
            /* SLIDE JS CLASSES (created with js) */
            .slidesjs-container {
                border: 1px solid #C7C7C7;
                }
            .slidesjs-pagination {
                position: absolute;
                top: -17%;
                left: 0px;
                width: 100%;
                text-align: center;        
                z-index: 50;      
                }
            .slidesjs-previous, .slidesjs-next {
                position: absolute;
                top: 41%;     
                z-index: 50;
                }
            .slidesjs-previous {
                display:none;
                left: -20%;
                } 
            .slidesjs-next {
                left: 110%;
                width: 0; 
                height: 0; 
                border-top: 30px solid transparent;
                border-bottom: 30px solid transparent;
                border-left: 30px solid $lightGrey;
                }  
            }
        img.allways-visible {
            opacity: 1 !important;
            display: block !important;    
            }    
        figcaption {
            position: absolute;
            bottom: -3em;    
            }    
        }
    
    [foo], &.figure-simple-hover {
        position: relative;
        
        &, img, object {
            opacity: 1 !important;
            }
        *.show-on-hover, *.hide-on-hover {
            position: absolute;
            top: 0;
            left: 0;
            display: block;
            -webkit-transition: $fadeOpacity;
            -moz-transition: $fadeOpacity;
            -o-transition: $fadeOpacity;
            -ms-transition: $fadeOpacity;
            transition: $fadeOpacity;
            }
        *.show-on-hover {
            opacity: 0 !important;
            }
        
        &:hover *.show-on-hover{
            opacity: 1!important;
            }
        *.hide-on-hover {
            opacity: 1 !important;
            } 
        &:hover *.hide-on-hover {
            opacity: 0!important;
            }
            
        }
    
    [foo], &.figure-slide-to-overlay {
        
        text-align:center;
        
        img.left-image, object.left-image {
            opacity: 1;
            z-index: 3;
            }
        img.right-image, object.right-image  {
            z-index: 1;
            }
        img, object {
            position: relative;
            -webkit-transition: $slideAnimation;
            -moz-transition: $slideAnimation;
            -o-transition: $slideAnimation;
            -ms-transition: $slideAnimation;
            transition: $slideAnimation;
            }
        figcaption {
            display: block;
            width: 100%;
            overflow: hidden;
            span {
                width: 50%;
                display: block;
                float: left;
                text-align: center;
                }
            p {
                width: 100%;
                display: block;
                }
            }
        &:hover, &.bring-together {
            
            img.left-image, object.left-image {
                margin-right: -25.25%;
                opacity: 0.65 !important;
                }
            img.right-image, object.right-image {
                margin-left: -25.204%;                   
                }    
            }    
        }
    
    &.figure-magnifier {
        position:relative;
        overflow:visible;

        .magnifier {
            position:absolute;
            overflow:hidden;
            top: 0;
            left:0;
            width: 250px;
            height: 250px;
            border-radius:50%;
            
            border: 1px solid $grey;
            background-repeat: no-repeat;
            background-color: $backgroundForOverlays;
            opacity:0.98;
            
            -webkit-box-shadow:  $shadowForOverlays;
            box-shadow:  $shadowForOverlays;
            
            display: none; 
            &:hover {
                cursor:none;
                }
            img {
                position: absolute;
                top: 0px;
                left: 0px;
                max-width:none;
                max-height:none;
                width:auto;
                height:auto;
                }
            
            }
        
        }
    
    /* Responsive Figures */
    
    &.three-figure-overlap  {
        width: auto;
        margin: 0 auto;
        
        img, object {
            display: inline-block;
            margin-left: 0%;
            width:auto;
            max-width: 30%;
            }
        img:first-child, object:first-child {
            margin-left: 0;    
            } 
        figcaption {
            margin-left: 0;
            }    
        }
    &.four-figure-overlap  {
        margin: 0 auto;
        
        img, object {
            display: inline-block;
            margin-left: -8%;
            width:auto;
            max-width: 30%;
            }
        img:first-child, object:first-child {
            margin-left: 0;    
            }
        figcaption {
            margin-left: 0;
            }    
        }
    &.figure-flipper {
        -moz-perspective: 1000;
        -webkit-perspective: 1000;
        -o-perspective: 1000;
        -ms-perspective: 1000;
        perspective: 1000;
        width: 100%;
        height: auto;
        text-align: center;

        img.figure-flipper-back, img.figure-flipper-front {
            border: 1px solid $lightGrey;
            border-radius: 1px;
            -moz-backface-visibility: hidden;
            -webkit-backface-visibility: hidden;
            -o-backface-visibility: hidden;
            -ms-backface-visibility: hidden;
            backface-visibility: hidden;
            margin:0;
            padding:0;
            }
        img.figure-flipper-front {
            z-index: 2;
            position: relative;
            top: 0;
            left: 0;
            }    
        img.figure-flipper-back {
            -moz-transform: rotateY(180deg);
            -webkit-transform: rotateY(180deg);
            -o-transform: rotateY(180deg);
            -ms-transform: rotateY(180deg);
            transform: rotateY(180deg);    
            
            position: absolute;
            top: 0;
            left: 0;
            }
        & > div.card {
            -moz-transition: 0.4s;                
            -webkit-transition: 0.4s;                
            -o-transition: 0.4s;                
            -ms-transition: 0.4s;                
            transition: 0.4s;
            
            -moz-transform-style: preserve-3d;
            -webkit-transform-style: preserve-3d;
            -o-transform-style: preserve-3d;
            -ms-transform-style: preserve-3d;
            transform-style: preserve-3d;
            
            margin-left: auto;
            margin-right: auto;
            max-width: 600px;
            }
        [foo], & > .card:hover, & > .card.hover {
            -moz-transform: rotateY(180deg);
            -webkit-transform: rotateY(180deg);
            -o-transform: rotateY(180deg);
            -ms-transform: rotateY(180deg);
            transform: rotateY(180deg);    
                
            }
        }
    
    
    & + p {
        margin-top: $larger;
        } 
    
    }
.figure-menu-undeselectable {
    menu {
        margin: $bigger auto $small;
        padding: 0 $normal;
        }
    p {
        margin-left: auto;
        margin-right: auto;
        font-size: $small;
        }
    & p + menu {
        margin-top: 0;
        }
    figure {
        height:0;
        display: none;
        opacity: 0;
        padding-top: $big;
        margin-top: 0;
        
        -webkit-transition: height 250ms ease-in-out, opacity 250ms ease-in-out;
        -moz-transition: height 250ms ease-in-out, opacity 250ms ease-in-out;
        -o-transition: height 250ms ease-in-out, opacity 250ms ease-in-out;
        -ms-transition: height 250ms ease-in-out, opacity 250ms ease-in-out;
        transition: height 250ms ease-in-out, opacity 250ms ease-in-out;
        }
    .visible {
        display: inline-block;
        height: inherit;
        opacity: 1 !important;
        }  
    }
.figure-menu {
    menu {
        margin: $bigger auto $small;
        padding: 0 $normal;
        }
    p {
        margin-left: auto;
        margin-right: auto;
        font-size: $small;
        }
    & p + menu {
        margin-top: 0;
        }
    figure, figure img, figure object {
        opacity: 0;
        }
    .visible, .visible * {
        opacity: 1 !important;
        }  
    }

.figure-tab-menu {
    menu {
        position: relative;
        top:1px;
        width: auto;
        margin: $bigger auto 0;
        padding: 0;
        background: $white;
        border: none;
        }

    p {
        text-align: center;
        margin-left: auto;
        margin-right: auto;
        }
    p + menu {
        margin-top: 0;
        }
    & menu + figure {
        background-color: $veryLightGrey;
        border: $border;
        -webkit-border-radius: 0px 0px $border-radius $border-radius;
        border-radius: 0px 0px $border-radius $border-radius;
        
        padding-top: $big;
        padding-bottom: $big;
        }
    & menu button, & menu .btn {
        margin: 0;
        -webkit-border-radius: 0px;
        border-radius: 0px;
        background: none;
        border-top: $border;
        border-left: $border;
        border-right: $border;
        -webkit-border-radius: $border-radius $border-radius 0px 0px;
        border-radius: $border-radius $border-radius 0px 0px;
        border-bottom: none;
        }
    & menu button:hover, & menu .btn:hover {
        background: rgba(0,0,0,0.15);
        }
    & menu button.selected, & menu .btn.selected {
        background-color: $hollColor;
        border-bottom: none;
        }
    }

.btn, button {
    font-size: $small;
    padding: $smallest $bigger;
    margin: $smallest;
    -webkit-border-radius: 3px;
    border-radius: 3px;
    
    border: 1px solid rgba(0,0,0,0.5);
    background-color: transparent;
    color: rgba(0,0,0,0.75);
    outline:none;
    
    -webkit-transition: $fadeButton;
    -moz-transition: $fadeButton;
    -o-transition: $fadeButton;
    -ms-transition: $fadeButton;
    transition: $fadeButton;
    
    
    &:hover {
        border-color: $textBlack;
        color: $textBlack;
        }
    &:active {
        background-color: rgba(0,0,0,0.95);
        color: $white
        }
    &.selected {
        background-color: $hollColor;
        color: $white;
        &:hover {
            background-color: $hollColorDark;
            }
                
        }
        
    &.small {;
        font-size: $smaller;
        }
    
    .bg-black & { 
        border-color: rgba(255,255,255,0.5);
        color: $textlightgrey;
        &:hover {
            border-color: $white;    
            color: $white;    
            }
        &:active {
            background-color: rgba(255,255,255,0.08);
            }
        }
        &.selected {
            background-color: $hollColor;
            color: $white;
            border-color: $hollColor;
            &:hover {
                background-color: $hollColorLight;
                border-color: $hollColorLight;
                } 
            }
        }


/* SLIDEJS ELEMENTS */
/* SlideJS not working wenn reload
.slidesjs-container, .slidesjs-control {
    position: static !important;
    height: auto !important;
    overflow: hidden;
    }
*/
.slidesjs-pagination {
    background: none;        
    .slidesjs-pagination-item {
        font-size: $normal;
        
        display: inline-block;
        padding: 0;
        margin: 0 $smallest;
        
        
        a, a:active, a:visited, a.active {
            font-size: $smallest;
            text-decoration: none;    
            
            width: $bigger;
            height: $bigger;
            display: block;
            vertical-align:middle;
                    
            color: transparent;
            background-color: $lightGrey;    
                
            -webkit-border-radius: 50%;
            border-radius: 50%;
            
            -webkit-transition: $fadeButton;
            -moz-transition: $fadeButton;
            -o-transition: $fadeButton;
            -ms-transition: $fadeButton;
            transition: $fadeButton;    
                
            &.active {
                background-color: $hollColor;
                }
            }
        a:hover {
            background-color: $veryLightGrey;
            }     
        }
    }        


